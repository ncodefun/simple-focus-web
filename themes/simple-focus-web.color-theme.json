				"keyword.operator",
				"storage.type.function.arrow",
				"keyword.operator.new.js",
				"keyword.operator.logical.and.media.css",
				"source.sass comment.punctuation"
			],
			"settings": {
				"foreground": "#9cf5ef"
			}
		},
		{
			"name": "grey-green: braces, parens, punctuation",
			"scope": [
				"meta.brace.round",
				"meta.parameters.js",
				"meta.tag",
				"meta.property-list",
				"meta.objectliteral.js",
				"meta.objectliteral punctuation.separator",
				"meta.array.literal punctuation.separator",
				"meta.brace.square",
				"meta.structure.dictionary.json",
				"meta.at-rule.keyframes.body.css",
				"meta.at-rule.media.body.css",
				"punctuation.definition.block",
				"punctuation.separator",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"punctuation.definition.block",
				"punctuation.separator.key-value",
				"punctuation.support.type.property-name",
			],
			"settings": {
				"foreground": "#83B0A8cc"
			}
		},
		{
			"name": "dark bluegreen: keywords for storage (var, function, class)",
			"scope": [
				"storage",
				"entity.name.tag",
				"meta.selector.css",
			],
			"settings": {
				"foreground": "#81a8a1"
			}
		},
		{
			"name": "medium-dark cyan: obj key quotes",
			"scope": [
				"meta.object-literal.key string.quoted punctuation.definition.string",
				"source.css meta.function",
				"punctuation.section.function"
			],
			"settings": {
				"foreground": "#6B9690"
			}
		},
		{
			"name": "control",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#688280"
			}
		},
		{
			"name": "darkest bluegreen: semi-colons...",
			"scope": [
				"punctuation.terminator.statement"
			],
			"settings": {
				"foreground": "#516260"
			}
		},
		{
			"name": "bright pale bluegreen: function parameters, class name, window.objects, attr",
			"scope": [
				"variable.parameter",
				"entity.other.attribute-name",
				"meta.object-literal.key",
				"meta.object-literal.key string.quoted.single.js",
				"meta.object-literal.key string.quoted.double.js",
				"meta.property-name.css",
				"punctuation.accessor",
				"variable.other.object.property",
				"variable.other.property.js",
				"support.variable.property.dom.js",
				"support.type.property-name.json",
				"support.type.property-name",
				"entity.other.attribute-selector" // attributes
				//"support.type.property-name.media.css"
			],
			"settings": {
				"foreground": "#cbffee"
			}
