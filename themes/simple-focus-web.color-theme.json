				"punctuation.definition.template-expression",
				"keyword.other.unit",
				"punctuation.terminator.rule.css",
				"string.js.taggedTemplate string.js",
				"punctuation.definition.string.template.end.js"
			],
			"settings": {
				"foreground": "#91b683"
			}
		},
		{
			"name": "dark pink: regex limits and flag",
			"scope": [
				"string.regexp punctuation.definition.string",
				"keyword.other",
				"punctuation.definition.group.regexp",
				"punctuation.definition.character-class.regexp"
			],
			"settings": {
				"foreground": "#a973ad"
			}
		},
		{
			"name": "darkest limegreen: css semi-colons",
			"scope": [
				"punctuation.terminator.rule.css"
			],
			"settings": {
				"foreground": "#647a67"
			}
		},
		{
			"name": "cyan: functions, classes; definition and calling",
			"scope": [
				"meta.definition",
				"meta.function-call",
				"variable.language.super",
				"entity.name.function.tagged-template",
