{
	"type": "dark",
	"colors": {
		"list.dropBackground": "#383b3d",
		"editor.background": "#1e1e1e",
		"editor.foreground": "#eeeeee",
		"editor.selectionBackground": "#5a5a36",
		"editor.inactiveSelectionBackground": "#686741",
		"editor.selectionHighlightBackground": "#686741",
		"editor.findMatchBackground": "#8b8a2f",
		"editor.findMatchHighlightBackground": "#6e6e46",
		"editorIndentGuide.background": "#404040",
		"editor.wordHighlightBackground": "#706f1300",
		"editor.wordHighlightStrongBackground": "#706f1300",
		"statusBar.foreground": "#f5f034",
		"statusBar.background": "#1d1d1e",
		"activityBarBadge.background": "#f5f034",
		"activityBarBadge.foreground": "#111111",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeeeeeff",
				"background": "#1e1e1eff"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		},
		{
			"name": "variables",
			"scope": [
				"support.variable",
				"variable"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "limegreen: values",
			"scope": [
				"constant",
				"support.constant.color",
				"markup.inline.raw",
				"string.quoted",
				"meta.property-value.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#b0ff91"
			}
		},
		{
			"name": "yellow: template value",
			"scope": [
				"string.template"
			],
			"settings": {
				"foreground": "#e7ff91"
			}
		},
		{
			"name": "darker limegreen: quotes, css units, template exp.",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.template-expression",
				"keyword.other.unit",
				"punctuation.terminator.rule.css"
			],
			"settings": {
				"foreground": "#91b683"
			}
		},
		{
			"name": "dark pink: regex limits and flag",
			"scope": [
				"string.regexp punctuation.definition.string",
				"keyword.other",
				"punctuation.definition.group.regexp",
				"punctuation.definition.character-class.regexp"
			],
			"settings": {
				"foreground": "#a973ad"
			}
		},
		{
			"name": "darkest limegreen: css semi-colons",
			"scope": [
				"punctuation.terminator.rule.css"
			],
			"settings": {
				"foreground": "#6a7a64"
			}
		},
		{
			"name": "bluegreen: functions, classes; definition and calling",
			"scope": [
				"meta.definition",
				"meta.function-call",
				"variable.language.super",
				"entity.name.function.tagged-template",
				//"support.type.property-name.json", looks nicer but less coherent
				"support.function"
			],
			"settings": {
				"foreground": "#8DFEF0"
			}
		},
		{
			"name": "medium-dark bluegreen: operator signs",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#9cf5ef"
			}
		},
		{
			"name": "medium-light bluegreen: literal obj/arr brackets, commas",
			"scope": [
				"meta.objectliteral punctuation.definition.block",
				"meta.objectliteral punctuation.separator",
				"meta.objectliteral",
				"meta.array.literal punctuation.separator",
				"meta.brace.square",
				"punctuation.separator.key-value",
				"meta.tag",
				"meta.property-list",
				"punctuation.support.type.property-name",
				"meta.structure.dictionary.json"
			],
			"settings": {
				"foreground": "#83B0A8"
			}
		},
		{
			"name": "dark bluegreen: keywords for storage (var, function, class, @param)",
			"scope": [
				"storage",
				"entity.name.tag",
				"meta.selector.css"
			],
			"settings": {
				"foreground": "#81a8a1"
			}
		},
		{
			"name": "darker bluegreen: obj key quotes",
			"scope": [
				"meta.object-literal.key string.quoted punctuation.definition.string",
				"source.css meta.function",
				"punctuation.section.function"
			],
			"settings": {
				"foreground": "#6B9690"
			}
		},
		{
			"name": "grey-bluegreen: braces, parens, punctuation, control",
			"scope": [
				"keyword.control",
				"meta.brace.round",
				"meta.parameters.js",
				"punctuation.definition.block",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#73887D"
			}
		},
		{
			"name": "darkest bluegreen: semi-colons...",
			"scope": [
				"punctuation.terminator.statement"
			],
			"settings": {
				"foreground": "#768173A0"
			}
		},
		{
			"name": "bright pale bluegreen: function parameters, class name, window.objects, attr",
			"scope": [
				"variable.parameter",
				"support.class",
				"entity.name.type.class",
				"entity.other.attribute-name",
				"meta.object-literal.key",
				"meta.object-literal.key string.quoted",
				"meta.property-name.css",
				"punctuation.accessor",
				"variable.other.property",
				"variable.other.object.property",
				"support.constant",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#cbffee"
			}
		},
		{
			"name": "yellow-green: this",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#edffcb"
			}
		},
		{
			"name": "pink: regex control and flags: ^ $",
			"scope": [
				"keyword.control.anchor.regexp"
			],
			"settings": {
				"foreground": "#f9bcff"
			}
		},
		{
			"name": "pale pink: regex char class",
			"scope": [
				"constant.other.character-class.set.regexp"
			],
			"settings": {
				"foreground": "#fddcff"
			}
		},
		{
			"name": "italic: function parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},

		{
			"name": "medium orange: @param",
			"scope": [
				"storage.type.class.jsdoc",
				"punctuation.definition.block.tag"
			],
			"settings": {
				"foreground": "#d4c287"
			}
		},
		{
			"name": "bright orange: {type}",
			"scope": [
				"entity.name.type.instance.jsdoc"
			],
			"settings": {
				"foreground": "#fff09e"
			}
		},
		{
			"name": "dark orange: jsdocs",
			"scope": [
				"comment.block.documentation.js"
			],
			"settings": {
				"foreground": "#8a8460"
			}
		},
		{
			"name": "darker orange: normal comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#796f53"
			}
		},
		{
			"name": "grey-brown: tag angled brakets",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#7a7a69"
			}
		},
		{
			"name": "bright pale yellow: css special keywords, like @keyframe n%",
			"scope": [
				"keyword.control.at-rule",
				"meta.at-rule.keyframes.body.css"
			],
			"settings": {
				"foreground": "#e0e990"
			}
		},

		{
			"name": "yellow: regex range",
			"scope": [
				"constant.other.character-class.range.regexp"
			],
			"settings": {
				"foreground": "#e7ff91"
			}
		},
		{
			"name": "purple: escape",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#c3bcff"
			}
		},
		{
			"name": "pink: regexp negation",
			"scope": [
				"keyword.operator.negation"
			],
			"settings": {
				"foreground": "#ff00bf"
			}
		}
	]
}